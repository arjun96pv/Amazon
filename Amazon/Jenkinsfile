pipeline{
    agent any
    
    stages{
        stage('clone_amazon_project'){
            steps{
                             git branch:'master' , url:'https://github.com/arjun96pv/Amazon.git'
            }
        }
         stage('compile'){
            steps{
               cd Amazon
               sh 'mvn compile'
            }
        }
        stage('test'){
            steps{
                cd Amazon
                sh 'mvn test'
            }
        }    
         stage('build'){
            steps{
                cd Amazon 
                sh 'mvn clean install'
            }   
        }
    }
        
}
